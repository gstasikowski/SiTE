    <system:String x:Key="MenuSaveNote">Save note</system:String>
    <system:String x:Key="MenuDeleteNote">Delete note</system:String>
    <system:String x:Key="MenuExit">Exit</system:String>
    <!-- Menu items - [Edit] -->
    <system:String x:Key="MenuEdit">Edit</system:String>
    <system:String x:Key="MenuCut">Cut</system:String>
    <system:String x:Key="MenuCopy">Copy</system:String>
    <system:String x:Key="MenuPaste">Paste</system:String>
    <system:String x:Key="MenuSelectAll">Select all</system:String>
    <system:String x:Key="MenuUndo">Undo</system:String>
    <system:String x:Key="MenuRedo">Redo</system:String>
    <system:String x:Key="MenuBold">Bold</system:String>
    <system:String x:Key="MenuItalic">Italic</system:String>
    <system:String x:Key="MenuHighlight">Highlight text</system:String>
    <system:String x:Key="MenuStrikethrough">Strikethrough</system:String>
    <system:String x:Key="MenuSubscript">Subscript</system:String>
    <system:String x:Key="MenuSuperscript">Superscript</system:String>
