    <system:String x:Key="MenuWordWrap">Word wrap</system:String>
    <system:String x:Key="MenuViewMode">Cycle editor mode</system:String>
    <!-- Menu items - [Settings] -->
    <system:String x:Key="MenuSettings">Settings</system:String>
